# Flex Insights Configuration
# Copy this file to .env and fill in your actual values

# Your Flex Insights workspace ID (found in Insights workspace URL)
ANALYTICS_WORKSPACE="xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Flex Insights API credentials
INSIGHTS_USERNAME="your_email@example.com"
INSIGHTS_PASSWORD="your_password"

# The report ID to execute (must return segment IDs in first column)
INSIGHTS_REPORT="1234567"